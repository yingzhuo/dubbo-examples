<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <dubbo:application name="de-consumer" version="1.0">
        <dubbo:parameter key="qos.enable" value="false"/>
    </dubbo:application>

    <dubbo:reference id="demoService" check="false" interface="de.service.DemoService"/>

    <beans profile="dev">
        <dubbo:registry protocol="zookeeper" address="127.0.0.1:2181"/>
    </beans>

    <beans profile="k8s">
        <dubbo:registry protocol="zookeeper" address="192.168.99.114:2181,192.168.99.114:2182,192.168.99.114:2183"/>
    </beans>

</beans>